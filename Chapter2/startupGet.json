  {
    "description":"模拟无参GET",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"这是无参的GET请求"
